{
  "deno.enable": true,
  "deno.codeLens.test": true,
  "deno.documentPreloadLimit": 2000,
  "deno.lint": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.formatOnSave": true,
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "cSpell.words": [
    "deepmerge",
    "freshui",
    "Mergeable",
    "preact"
  ],
  "css.customData": [
    ".vscode/tailwind.json"
  ]
}
